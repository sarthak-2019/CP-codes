// https://leetcode.com/problems/unique-paths/


// Approach 1 [Dp Solution]

class Solution {
public:
    int dp[105][105];
    int cal(int m,int n){
        
        if(m==1||n==1)
            return 1;
        if(m<1||n<1)
            return 0;
        if(dp[m][n]!=-1)
            return dp[m][n];
        int x=cal(m-1,n);
        int y=cal(m,n-1);
        return dp[m][n]=x+y;
    }
    int uniquePaths(int m, int n) {
        memset(dp,-1,sizeof(dp));
        return cal(m,n);
    }
};

